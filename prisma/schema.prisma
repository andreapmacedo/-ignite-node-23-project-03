generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    String @id @default(uuid())
  name  String
  email String @unique

  @@map("users") // Modifica o nome da tabela no banco de dados. Neste caso o nome seria User e passa a ser users
}
